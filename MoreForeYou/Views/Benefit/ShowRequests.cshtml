        //alert(document.getElementById("employeeNameee").innerText);
        document.getElementById("employeeDepartment").innerText = document.getElementById("employeeDepartment" + i).value;
        document.getElementById("employeePosition").innerText = document.getElementById("employeePosition" + i).value;
        document.getElementById("employeeEmail").innerText = document.getElementById("employeeEmail" + i).value;
        document.getElementById("employeeSapNumber").innerText = document.getElementById("employeeSapNumber" + i).value;
        document.getElementById("employeeEmployeeNumber").innerText = document.getElementById("employeeEmployeeNumber" + i).value;
        document.getElementById("employeeCompanyCode").innerText = document.getElementById("employeeCompanyCode" + i).value;
        //document.getElementById("employeeBirthDate").innerText = document.getElementById("employeeBirthDate" + i).value;
        document.getElementById("employeePhoto").src = "http://20.86.97.165/more4u/images/userProfile/" + document.getElementById("employeephoto" + i).value;
        document.getElementById("employeeGender").innerText = document.getElementById("employeeGender" + i).value;
        document.getElementById("employeeMartialStatus").innerText = document.getElementById("employeeMartialStatus" + i).value;
        document.getElementById("From").innerText = document.getElementById("From" + i).value;
        document.getElementById("To").innerText = document.getElementById("To" + i).value;
       // alert(filesCount);
        if (filesCount > 0) {
            var text = document.createElement("h4");
            text.setAttribute("class", "displayDocuments");
            text.textContent = "Uploaded Documents";
            var card = document.getElementById("cardBody");
            card.innerHTML="";
            card.appendChild(text);
            for (var fileIndex = 0; fileIndex < filesCount; fileIndex++) {
                var fileId = "document" + i + "number" + fileIndex;
                var doc = document.getElementById(fileId);
                var row = document.createElement("div");
                row.setAttribute("class", "row pt-2 displayDocuments");
                var img = document.createElement("img");
                var filePath = doc.value;
                //img.setAttribute("src", "http://20.86.97.165/more4u/BenefitRequestFiles/" + filePath);
                img.setAttribute("src",filePath);

                img.setAttribute("style", "width:400px;height:250px");
                img.setAttribute("class", "m-2");
                var hr = document.createElement("hr");
                row.appendChild(hr);
                row.appendChild(img);
                card.appendChild(row);
            }
        }
        var actionId = "MyAction" + i;
        var myAction = document.getElementById(actionId);
        if (myAction != null) {
            var divApproval = document.createElement("div");
            divApproval.setAttribute("id", "divApprovalId");
            divApproval.setAttribute("style", "padding:40px;font-size:14px");
            var approvedNameId = "ResponserAction" + i;
            var ReplayDateId = "ReplayDate" + i;
            var ReplayMessageId = "ReplayMessage" + i;
            var lblName = document.createElement("label");
            var lblReplayDate = document.createElement("label");
            if (myAction.value == "Approved") {
                lblName.setAttribute("style", "text-align:center; display:block; color:green; margin-bottom:10px");
            }
            else {
                lblName.setAttribute("style", "text-align:center; display:block; color:red; margin-bottom:10px");

            }
            lblName.textContent = document.getElementById(approvedNameId).value + " " + myAction.value + " this request";
            lblReplayDate.setAttribute("style", "display:block; margin-bottom:10px");
            lblReplayDate.textContent = "Date:- " + document.getElementById(ReplayDateId).value;
            var lblReplayMessage = document.createElement("label");
            lblReplayMessage.textContent = "";
            if (document.getElementById(ReplayMessageId) != null) {
                lblReplayMessage.textContent = "Notes:- " + document.getElementById(ReplayMessageId).value;
            }
            var myApprovalCard = document.getElementById("myApprovalCard");
            myApprovalCard.innerHTML=""
            divApproval.appendChild(lblName);
            divApproval.appendChild(lblReplayDate);
            divApproval.appendChild(lblReplayMessage);
            myApprovalCard.appendChild(divApproval);
        }
        document.getElementById("lmdetails").hidden = false;
        document.getElementById("lmdetailss").hidden = true;
                    //document.getElementById("dlm").hidden = true;
    }
</script>
<script type="text/javascript">
    function AddResponse()
    {
        var url = '@Url.Content("~/")' + "more4u/Benefit/AddResponse";
        var requestId = document.getElementById("request").value;
        var status = document.getElementById("StatusValue").value;
        var message = document.getElementById("message").value;
        $.getJSON(url, { requestWorkflowId: requestId, status: status, message: message }, function (data)
        {
            if (data == true)
            {
                var eleId = "requeststatus" + requestId;
                var actionArea = "actionArea" + requestId;
                if (status == 1) {
                    //document.getElementById(requestId).innerHTML = "<b style='color:green'>Approved</b>";
                    //document.getElementById("requeststatus" + requestId).innerHTML = "<b style='color:green'>Approved</b>";
                    swal({
                        title: "Approved!",
                        text: "Sucess Process!",
                        icon: "success",
                    });
                    document.getElementById(eleId).innerHTML = "<b style='color:green'>Approved</b>";
                    document.getElementById(actionArea).innerHTML = "<b style='color:green'>Approved</b>";
                    document.getElementById("message").value = "";

                }
                else
                {
                    swal({
                        title: "Rejected!",
                        text: "Sucess Process!",
                        icon: "error",
                    });
                    document.getElementById(eleId).innerHTML = "<b style='color:red'>Rejected</b>";
                    document.getElementById(actionArea).innerHTML = "<b style='color:red'>Rejected</b>";
                    document.getElementById("message").value = "";
                }
            }
            else {
                swal({
                    title: "error!",
                    text: "Failed Process!",
                    icon: "error",
                });

            }
        });
    }

    function addGroupmembers(members)
    {
        for (var member in members)
        {
            var mainDiv = document.getElementById("groupMembers");
            var memberDetails = document.createElement("div");
            memberDetails.innerHTML = "<b>" + member["employeeName"] + "</b> </br>" + member["employeePosition"] + "\"";
        }
    }
</script>
<script>

    function showMemberModal(members, i, participantIndex) {
        document.getElementById("memberFrom").innerText = document.getElementById("From" + i).value;
        document.getElementById("memberTo").innerText = document.getElementById("To" + i).value;
        document.getElementById("requestIndex").value = i;
        document.getElementById("memberName").innerText = members["EmployeeName"];
        document.getElementById("memberDepartment").innerText = members["DepartmentName"];
        document.getElementById("memberPosition").innerText = members["PositionName"];
        document.getElementById("memberEmail").innerText = members["Email"];
        document.getElementById("memberNumber").innerText = members["EmployeeNumber"];
        document.getElementById("memberSapNumber").innerText = members["SapNumber"];
        //document.getElementById("memberBirthDate").innerText = members["BirthDate"];
        document.getElementById("memberGender").innerText = members["Gender"];
        document.getElementById("memberMartialStatus").innerText = members["MaritalStatus"];
        document.getElementById("memberCompany").innerText = members["Company"];
        document.getElementById("memberProfile").src = "http://20.86.97.165/more4u/images/userProfile/" + members["ProfilePicture"];
        $('#memberModal').modal('show');

    }

    $(document).ready(function () {

       

        $("#memberModal").on('show.bs.modal', function (event) {

            var modal = $(this);
            var i = document.getElementById('requestIndex').value;
            if (document.getElementById("requestMessage" + i).value != "false") {
                var messageDiv = document.createElement("div");
                messageDiv.setAttribute("class", "col-lg-12 mb-1 mt-1 modalMessage");
                var h6 = document.createElement("h6");
                h6.textContent = "Message";
                var message = document.createElement("p");
                message.setAttribute("class", "text-muted");
                message.textContent = document.getElementById("requestMessage" + i).value;
                messageDiv.appendChild(h6);
                messageDiv.appendChild(message);
                modal.find('.modal-body .requestInfo').append(messageDiv);
            }

        });
        $("#exampleModal").on('show.bs.modal', function (event) {
            var anchor = $(event.relatedTarget) // Button that triggered the modal
            var modal = $(this)
            modal.find('#memberName').text(anchor.data('name'))
            modal.find('#memberDepartment').text(anchor.data('department'))
            modal.find('#memberPosition').text(anchor.data('position'))
            modal.find('#memberCollar').text(anchor.data('collar'))
            //modal.find('#memberDateBirth').text(anchor.data('birthdate'))
            modal.find('#memberEmail').text(anchor.data('email'))
            modal.find('#memberWorkDuration').text(anchor.data('workduration'))
            modal.find('#memberNumber').text(anchor.data('number'))
            modal.find('#memberCompany').text(anchor.data('company'))
            modal.find('#memberSapNumber').text(anchor.data('sapnumber'))
        });
        $("#MessageModal").on('show.bs.modal', function (event) {

            var button = $(event.relatedTarget) // Button that triggered the modal
            var modal = $(this)
            var id = button.data('request')
            var st = button.data('status')
            modal.find('#request').val(id)
            modal.find('#StatusValue').val(st)
        });
    });


    function clearElements() {
        $(".modalMessage").remove();
        $(".displayDocuments").remove();
        document.getElementById("message").value = "";
        $("#divApprovalId").remove();
    }
</script>
