<div class="row">
@if (Model.Requests != null && Model.Requests.Count != 0)
    {
        <div class="col-md-8 mt-4">
          <div class="card h-100 mb-4">
            <div class="card-header pb-0 px-3">
              <h6 class="mb-0">Requests</h6>
              <p class="text-sm mb-0">
                <i class="fa fa-check text-info" aria-hidden="true"></i>
                <span class="font-weight-bold ms-1">Searching</span> Results
              </p>
            </div>
             <div class="card-body pt-4 p-3">
              <ul class="list-group">
            <!--list of requests-->
            @for (index = 0; index < Model.Requests.Count; index++)
    {

        employeeName = "employeeName" + index;
        employeeNumber = "employeeNumber" + index;
        employeePhoto = "employeephoto" + index;
        employeePosition = "employeePosition" + index;
        employeeDepartment = "employeeDepartment" + index;
        employeeBirthDate = "employeeBirthDate" + index;
        employeeCollar = "employeeCollar" + index;
        employeeCompanyCode = "employeeCompanyCode" + index;
        employeeEmployeeNumber = "employeeEmployeeNumber" + index;
        employeeSapNumber = "employeeSapNumber" + index;
        employeeEmail = "employeeEmail" + index;
        requestMessage = "requestMessage" + index;
        requestStatus = "requeststatus" + Model.Requests[index].RequestWorkflowId;
        To = "To" + index;
        From = "From" + index;
        employeeMartialStatus = "employeeMartialStatus" + index;
        employeeGender = "employeeGender" + index;
        requestBenefitType = "requestBenefitType" + index;
        trId = "tr" + Model.Requests[index].RequestNumber;
        actionArea = "actionArea" + Model.Requests[index].RequestWorkflowId;
        warningMessageId = "warningMessageId" + Model.Requests[index].RequestWorkflowId;
        filesCount = "filesCount" + index;
        MyAction = "MyAction" + index;
        ReplayDate = "ReplayDate" + index;
        ReplayMessage = "ReplayMessage" + index;
        ResponserAction = "ResponserAction" + index;
                            @if (Model.Requests[index].WarningMessage != null)
                            {
                                <div style="margin-left:30px"><i class="bi bi-exclamation-circle text-warning"></i><span style="color:red; padding-left:5px">@Model.Requests[index].WarningMessage</span></div>
                            }
                           
                                <div class="" id="@index">
                                    @if (Model.Requests[index].MyAction != null)
                                    {
                                        <input type="hidden" id="@MyAction" value="@Model.Requests[index].MyAction.action" />
                                        <input type="hidden" id="@ReplayDate" value="@Model.Requests[index].MyAction.ReplayDate" />
                                                                          
                                       
                                    }
                                </div>

                            <li class="list-group-item border-0 d-flex p-4 mb-2 bg-gray-100 border-radius-lg" id=@trId>
                                <div class="d-flex flex-column">
                                    <h6 class="mb-3 text-sm">@Model.Requests[index].BenefitName</h6>
                                @*       <hr class="horizontal dark my-sm-4"> *@
                                    <span class="mb-2 text-xs">Employee: <span class="text-dark font-weight-bold ms-sm-2" id="requestNumber">@Model.Requests[index].CreatedBy.UserName</span></span>
                                    @if (Model.Requests[index].BenefitType == "Group")
                                    {
                                        var FullParticipantsData = JsonSerializer.Serialize(Model.Requests[index].FullParticipantsData);
                                        for (int memberIndex = 0; memberIndex < Model.Requests[index].FullParticipantsData.Count; memberIndex++)
                                        {
                                            var id = "group" + index + "member" + memberIndex;
                                            <input type="hidden" id=@id value=@Model.Requests[index].FullParticipantsData[memberIndex].UserName />
                                        }
                                    }
                                    @if (Model.Requests[index].DocumentsPath != null)
                                    {
                                        <input type="hidden" id="@filesCount" value="@Model.Requests[index].DocumentsPath.Length" />
                                        for (int documentIndex = 0; documentIndex < Model.Requests[index].DocumentsPath.Length; documentIndex++)
                                        {
                                            var documentId = "document" + index + "number" + documentIndex;
                                            <input type="hidden" id=@documentId value=@Model.Requests[index].DocumentsPath[documentIndex] />
                                        }
                                    }
                                    else
                                    {
                                        <input type="hidden" id="@filesCount" value="0" />
                                    }
                                    <span class="mb-2 text-xs">Request Date: <span class="text-dark ms-sm-2 font-weight-bold">@Model.Requests[index].Requestedat</span></span>
                                @if (Model.Requests[index].status == "Pending")
                                {
                                       <span class="mb-2 text-xs">Status: <span class="text-dark ms-sm-2 font-weight-bold" style="color: #1aa3ff;" id=@requestStatus>@Model.Requests[index].status</span></span>
                                }
                                else if (Model.Requests[index].status == "InProgress")
                                {
                                        <span class="mb-2 text-xs">Status: <span class="text-dark ms-sm-2 font-weight-bold" style="color: #ffd614;" id=@requestStatus>@Model.Requests[index].status</span></span>                                     
                                }
                                else if (Model.Requests[index].status == "Approved")
                                {
                                      <span class="mb-2 text-xs">Status: <span class="text-dark ms-sm-2 font-weight-bold" style="color: #00ff00;" id=@requestStatus>@Model.Requests[index].status</span></span>
                   
                                }
                                else if (Model.Requests[index].status == "Rejected")
                                {
                                        <span class="mb-2 text-xs">Status: <span class="text-dark ms-sm-2 font-weight-bold" style="color: red;" id=@requestStatus>@Model.Requests[index].status</span></span>
                                }
                                else if (Model.Requests[index].status == "Cancelled")
                                {
                                        <span class="mb-2 text-xs">Status: <span class="text-dark ms-sm-2 font-weight-bold" style="color: gray;" id=@requestStatus>@Model.Requests[index].status</span></span>                                       
                                }
                                    <span class="text-xs">Required Date: <span class="text-dark ms-sm-2 font-weight-bold">@Model.Requests[index].From</span></span>
